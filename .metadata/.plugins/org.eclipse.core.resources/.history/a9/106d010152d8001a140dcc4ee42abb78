package initial;

import io.cucumber.java.en.Given;
import io.cucumber.java.en.Then;
import io.cucumber.java.en.When;

public class CucumberInicial {

	@Given("que criei o arquivo corretamente")
	public void que_criei_o_arquivo_corretamente() {
		System.out.println("Foi criado o arquivo correntamente");
	}

	@When("executa-lo")
	public void executa_lo() {
		System.out.println("Foi executado o arquivo");
	}

	@Then("a especificação deve finalizar com sucesso")
	public void a_especificação_deve_finalizar_com_sucesso() {
		System.out.println("a especificação finalizou com sucesso");
	}
}
